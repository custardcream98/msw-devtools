{
  "name": "playground-vue",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently \"msw-devtools-server -o ./mock -r\" \"vite\"",
    "prebuild": "pnpm --filter @custardcream/msw-devtools build",
    "build": "pnpm prebuild && run-p type-check:build \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "type-check:build": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@tanstack/vue-query": "^5.59.13",
    "vue": "^3.5.12"
  },
  "devDependencies": {
    "@custardcream/msw-devtools": "workspace:*",
    "@custardcream/msw-devtools-server": "workspace:*",
    "@rushstack/eslint-patch": "^1.10.4",
    "@tsconfig/node20": "^20.1.4",
    "@types/node": "^20.16.11",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/tsconfig": "^0.5.1",
    "concurrently": "^9.0.1",
    "eslint": "^8.57.1",
    "eslint-plugin-vue": "^9.29.0",
    "msw": "^2.6.0",
    "npm-run-all2": "^6.2.3",
    "prettier": "^3.3.3",
    "typescript": "~5.4.5",
    "vite": "^5.4.8",
    "vue-tsc": "^2.1.6"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
