{
  "$schema": "https://json.schemastore.org/package",
  "name": "@custardcream/msw-devtools-server",
  "type": "module",
  "version": "0.7.0",
  "description": "Websocket server for @custardcream/msw-devtools",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "types": "dist/index.d.ts",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "files": [
    "dist",
    "bin"
  ],
  "bin": {
    "msw-devtools-server": "bin/index.cjs"
  },
  "scripts": {
    "build": "rollup -c --bundleConfigAsCjs",
    "lint": "eslint --cache",
    "lint:fix": "pnpm lint --fix",
    "type-check": "tsc --noEmit --skipLibCheck",
    "build:watch": "pnpm build --watch",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "chokidar": "^4.0.1",
    "commander": "^12.1.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@eslint/compat": "^1.1.1",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node": "^22.5.5",
    "@types/ws": "^8.5.12",
    "@vitest/coverage-v8": "^2.1.1",
    "@vitest/ui": "^2.1.1",
    "eslint": "^9.9.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-testing-library": "^6.3.0",
    "msw": "^2.4.8",
    "rollup": "^4.21.3",
    "rollup-plugin-delete": "^2.1.0",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-tsconfig-paths": "^1.5.2",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.0.1",
    "vitest": "^2.1.1"
  },
  "peerDependencies": {
    "msw": ">=2.0.0"
  },
  "author": "shiwoo.park <custardcream@kakao.com>",
  "bugs": {
    "url": "https://github.com/custardcream98/msw-devtools/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/custardcream98/msw-devtools.git"
  },
  "keywords": [
    "msw",
    "devtools",
    "mock-service-worker",
    "testing",
    "development",
    "QA",
    "framework-agnostic",
    "webpack",
    "webpack-plugin"
  ],
  "license": "Apache-2.0"
}
