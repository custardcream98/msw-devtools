  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/custardcream98/msw-devtools.git"
  },
  "keywords": [
    "msw",
    "devtools",
    "mock-service-worker",
    "testing",
    "development",
    "QA",
    "framework-agnostic",
    "webpack",
    "webpack-plugin"
