{
  "$schema": "https://json.schemastore.org/package",
  "name": "core",
  "private": true,
  "version": "0.7.0",
  "type": "module",
  "files": [
    "src"
  ],
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    },
    "./vite/events": {
      "types": "./vite/events.d.ts"
    }
  },
  "types": "./src/index.ts",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "scripts": {
