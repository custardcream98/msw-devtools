    "@vitest/ui": "^2.1.1",
    "@eslint/compat": "^1.1.1",
    "eslint": "^9.9.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-testing-library": "^6.3.0",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.0.1",
    "vitest": "^2.1.1",
    "vite": "^5.4.8"
  }
