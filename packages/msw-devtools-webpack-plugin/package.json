{
  "$schema": "https://json.schemastore.org/package",
  "name": "@custardcream/msw-devtools-webpack-plugin",
  "version": "0.7.0",
  "description": "Webpack plugin for @custardcream/msw-devtools",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "types": "dist/index.d.ts",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "lint": "eslint --cache",
    "lint:fix": "pnpm lint --fix",
    "type-check": "tsc --noEmit --skipLibCheck",
    "build:watch": "pnpm build --watch",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@eslint/compat": "^1.1.1",
    "@types/node": "^22.5.5",
    "@vitest/coverage-v8": "^2.1.1",
    "@vitest/ui": "^2.1.1",
    "core": "workspace:*",
    "eslint": "^9.9.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-testing-library": "^6.3.0",
    "msw": "^2.4.8",
    "tsup": "^8.3.0",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.0.1",
    "vitest": "^2.1.1",
    "webpack": "^5.95.0"
  },
  "peerDependencies": {
    "msw": ">=2.0.0"
  },
  "author": "shiwoo.park <custardcream@kakao.com>",
  "bugs": {
    "url": "https://github.com/custardcream98/msw-devtools/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/custardcream98/msw-devtools.git"
  },
  "keywords": [
    "msw",
    "devtools",
    "mock-service-worker",
    "testing",
    "development",
    "QA",
    "framework-agnostic",
    "webpack",
    "webpack-plugin"
  ],
  "license": "Apache-2.0"
}
